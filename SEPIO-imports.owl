                                                                                          "person:Chris Stoeckert"@en ;
                                             <http://purl.obolibrary.org/obo/IAO_0000119> "OBI_0000042"@en ,
                                                                                          "group:OBI"@en ;
                                             <http://purl.obolibrary.org/obo/IAO_0000412> <http://purl.obolibrary.org/obo/obi.owl> ;
                                             rdfs:label "data set"@en .


